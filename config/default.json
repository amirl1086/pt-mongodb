{  
  "server": {
    "host": "main_db",
    "port": 27017,
    "mainDB_Name": "main",
    "adminDB_Name": "admin"
  },
  "remotes": {
    "rabbitmq": {
      "username": "rabbit_user",
      "password": "rabbit_password",
      "host": "rabbitmq",
      "exchangeName": "maindb-exchange",
      "exchangeStrategy": "direct",
      "queueName": "user-queue",
      "consumerTag": "user-consumer",
      "key": "user"
    }
  },
  "auth": {
    "roles": ["user", "moderator", "admin"]
  }
}